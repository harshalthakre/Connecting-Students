<!-- start display -->
<div class="content" style="">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">NOTICE</h4>
                                <p class="category">Here is the list of imp notice</p>
                            </div>
                            <div class="content table-responsive table-full-width">
                                <table class="table table-hover table-striped">
                                  <thead>
                                      <th>Number</th>
                                    <th>Title</th>
                                    <th>Description</th>
                                    <th>Date</th>

                                  </thead>


                                    <tbody>
                                      <%for(count=0;count<notice.length;count++){%>
                                        <tr>
                                          <td><%=count+1%></td>
                                          <td><%=notice[count].title%></td>
                                          <td>$36,738</td>
                                          <td><%=new Date()%></td>

                                        </tr>
                                          <% if(count>=notice.length){
                                            break;
                                          }%>
                                        <tr>
                                          <td><%=count+1+1%></td>
                                          <td><%=notice[count].title%></td>
                                          <td>$23,789</td>
                                          <td><%new Date()%></td>

                                        </tr>

                                      <%}%>


                                    </tbody>
                                </table>

                            </div>
                        </div>
                </div>
        </div>
    </div>
</div>
