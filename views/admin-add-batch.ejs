<% include partials/admin-nav.ejs %>
        <div class="content">
          <p style="color:red"><%=msg%></p>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-2">
                    </div>
                    <div class="col-md-8">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Add New Batch</h4>
                            </div>
                            <div class="content">
                                <form method="post" action='/admin-add-batch'>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Roll String: </label>
                                                <input type="text" class="form-control" placeholder="2014BCS" name="rollString">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Total Strength</label>
                                                <input type="text" class="form-control" placeholder="120" name="totalStrength">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label>Department</label>
                                                  <select name="department" class="form-control" placeholder="Select Department">
                                                    <option value=null>Select Department</option>
                                                    <option value="cse">Computer science and engineering</option>
                                                    <option value="mech">Mechanical engineering</option>
                                                    <option value="it">Information Technology</option>
                                                    <option value="civil">Civil enginerring</option>
                                                    <option value="electrical">Electrical engineering</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Year</label>
                                                <select name="year" class="form-control" placeholder="Select Year">
                                                  <option value=null>Select Year</option>
                                                  <option value="2014">2014-15</option>
                                                  <option value="2015">2015-16</option>
                                                  <option value="2016">2016-17</option>
                                                  <option value="2017">2017-18</option>
                                                  <option value="2018">2018-19</option>
                                              </select>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Wing</label>
                                                <select name="wing" class="form-control" placeholder="Select Wing">
                                                  <option value=null>Select Year</option>
                                                  <option value="btech">B.Tech</option>
                                                  <option value="mtech">M.Tech</option>
                                                  <option value="diploma">Diplomas</option>
                                              </select>
                                            </div>
                                        </div>

                                    </div>



                                    <button type="submit" class="btn btn-info btn-fill pull-right">Update Profile</button>
                                    <div class="clearfix"></div>
                                </form>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
        </div>


        <footer class="footer">
            <div class="container-fluid">
                <nav class="pull-left">
                    <ul>
                        <li>
                            <a href="#">
                                Home
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                Company
                            </a>
                        </li>
                        <li>
                            <a href="#">
                                Portfolio
                            </a>
                        </li>
                        <li>
                            <a href="#">
                               Blog
                            </a>
                        </li>
                    </ul>
                </nav>
                <p class="copyright pull-right">
                    &copy; <script>document.write(new Date().getFullYear())</script> <a href="#">SociEduTek</a>,By WCE,Sangli
                </p>
            </div>
        </footer>

    </div>
</div>


</body>

    <!--   Core JS Files   -->
    <script src="/assets/admin-nav/js/jquery-1.10.2.js" type="text/javascript"></script>
	<script src="/assets/admin-nav/js/bootstrap.min.js" type="text/javascript"></script>

	<!--  Checkbox, Radio & Switch Plugins -->
	<script src="/assets/admin-nav/js/bootstrap-checkbox-radio-switch.js"></script>

	<!--  Charts Plugin -->
	<script src="/assets/admin-nav/js/chartist.min.js"></script>

    <!--  Notifications Plugin    -->
    <script src="/assets/admin-nav/js/bootstrap-notify.js"></script>

    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>

    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
	<script src="/assets/admin-nav/js/light-bootstrap-dashboard.js"></script>

	<!-- Light Bootstrap Table DEMO methods, don't include it in your project! -->
	<script src="/assets/admin-nav/js/demo.js"></script>

</html>
