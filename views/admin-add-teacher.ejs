                    <div class="col-md-8">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Add New Teacher</h4>
                            </div>
                            <div class="content">
                                <form method="post" action='/admin-add-teacher'>


                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>First Name</label>
                                                <input type="text" class="form-control" placeholder="Kiran" name="fname">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label>Last Name</label>
                                                <input type="text" class="form-control" placeholder="Kamble" name="lname">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label>Department</label>
                                                  <select name="department" class="form-control" placeholder="Select Department">
                                                    <option value=null>Select Department</option>
                                                    <option value="cse">Computer science and engineering</option>
                                                    <option value="mech">Mechanical engineering</option>
                                                    <option value="it">Information Technology</option>
                                                    <option value="civil">Civil enginerring</option>
                                                    <option value="electrical">Electrical engineering</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-6">
                                          <div class="form-group">
                                              <label>Email</label>
                                              <input type="email" class="form-control" placeholder="harshalthakre.07@gmail.com" name="email">
                                          </div>
                                        </div>
                                        <div class="col-md-6">
                                          <div class="form-group">
                                              <label>User Name</label>
                                              <input type="text" class="form-control" placeholder="kk7" name="uname">
                                          </div>
                                        </div>

                                    </div>



                                    <button type="submit" class="btn btn-info btn-fill pull-right">Add Teacher</button>
                                    <div class="clearfix"></div>
                                </form>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
